package com.tutorialspoint;

import static org.junit.Assert.*;

import java.sql.SQLException;
import java.util.List;

import org.junit.Test;

public class UserDaoTest {
	
	UserDao userDao = new UserDao();
	
	List<User> userList = null;
	User user = new User(1,"Joe","Software Engineer");
	private final int EXPECTED_USER_ID = 1;
	private User ACTUAL_USER_ID = null;
	

	@Test
	public void testGetOneUser() throws SQLException {
		userList = UserDao.getUser(EXPECTED_USER_ID);
		ACTUAL_USER_ID = userList.get(1);
		assertEquals(ACTUAL_USER_ID, ACTUAL_USER_ID);	}

}
